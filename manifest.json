{
  "manifest_version": 2,
  "name": "Leeter",
  "description": "Tracks all your LeetCode submissions",
  "version": "0.1.0",
  "icons": {
    "128": "assets/icon.png"
  },
  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/problems/*"],
      "js": ["js/contentScript.js"],
      "run_at": "document_idle"
    }
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "downloads",
    "storage",
    "http://localhost:3000/"
  ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/js/bootstrap.bundle.min.js 'unsafe-inline'; object-src 'self'"
}
